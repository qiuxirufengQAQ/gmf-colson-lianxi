                        <groupId>net.winroad</groupId>
                        <artifactId>wrdoclet</artifactId>
                        <version>1.2.3</version>
                    </docletArtifact>
                    <useStandardDocletOptions>false</useStandardDocletOptions>
                    <additionalparam>

                        -charset utf-8
                        -d
